<%-include("header")-%>

<h1 id="title">quizzes</h1>

<div class="names">
    <% if(q.length ==0){ %>
        <div id="title"><h1>no quizzes</h1></div>
        <h4>create you own quiz by pressing <a href="/<%=uname%>/compose" style="color:blue">here</a></h4>
    <%}else{ %>

    <% q.forEach(function(qq){%>
        <div class="quizlist" id="start" style="cursor: pointer;" onclick="location.href='/questions/<%=uname%>/<%=qq.name%>';">
            
            <div class="namee"><h3 class="na"><%=qq.name%></h3></div>
                <!-- <div class="start"><a href="/<%=//qq.name%>" class="n">start</a></div> -->
                <!-- <div class="deletequiz"> -->
                    <div class="delete">
                        <a href="/<%=qq.name%>/delete" class="dele">delete</a>
                    </div>
                <!-- </div> -->
        </div>
    <%}) %> 
    <%}%>
</div>

<%-include("footer")-%>